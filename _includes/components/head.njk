<head>
<!-- Global site tag (gtag.js) - Google Analytics -->
<script async src="https://www.googletagmanager.com/gtag/js?id=G-PFCPGB92VY"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', 'G-PFCPGB92VY');
</script>
  <meta charset="utf-8">
  <meta name="robots" content="index, follow">
  <link rel="dns-prefetch" href="https://identity.netlify.com">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{ settings.site_title }} | {{ title or renderData.title or metadata.title }}</title>
  <link rel="icon" href="{{ settings.site_favicon }}" sizes="192x192">
  <link rel="apple-touch-icon" href="{{ settings.site_favicon }}">
  <meta name="msapplication-TileImage" content="{{ settings.site_favicon }}">
  <meta name="dc.title" content="{{ settings.site_title }} | {{ title or renderData.title or metadata.title }}">
  <meta name="dc.description" content="{{ settings.site_tagline }}">
  <meta name="dc.language" content="en_CA">
  <meta name="description" content="{{ settings.site_tagline }}">
  <meta property="og:description" content="{{ settings.site_tagline }}">
  <meta property="og:site_name" content="{{ settings.site_title }}">
  <meta property="og:title" content="{{ settings.site_title }} | {{ title or renderData.title or metadata.title }}">
  <meta property="og:image" content="{{ settings.social_image }}">
  <meta property="og:image:secure_url" content="{{ settings.social_image }}">
  <meta name="twitter:description" content="{{ settings.site_tagline }}">
  <meta name="twitter:title" content="{{ settings.site_title }} | {{ title or renderData.title or metadata.title }}">
  <meta name="twitter:image" content="{{ settings.social_image }}">
  <script async src="https://identity.netlify.com/v1/netlify-identity-widget.js"></script>
  <link rel="stylesheet" href="/assets/css/tailwind.css">
  {% set css %}
    {% include "assets/css/typekit.css" %}
    {% include "assets/css/splide.min.css" %}
  {% endset %}
  <style>{{ css | cssmin | safe }}</style>
  {% set js %}
    {% include "assets/js/jquery.min.js" %}
    {% include "assets/js/inline.js" %}
    {% include "assets/js/splide.min.js" %}
  {% endset %}
  <script>{{ js | jsmin | safe }}</script>
<link rel="preconnect" href="https://fonts.gstatic.com">
<link href="https://fonts.googleapis.com/css2?family=Archivo+Black&family=Open+Sans:ital,wght@0,600;0,700;1,400&display=swap" rel="stylesheet">
</head>
